{
  "manifest_version": 2,
 
  "name": "NoBias",
  "version": "0.1.0",
  "description": "Removes your biases when finding people to recruit!",
  "icons": { "128": "icon_128.png" },

  "browser_action":{
    "default_title": "NoBias",
    "default_icon": "icon_19.png"
  },
 
  "content_scripts": [{
    "css": ["css/linkedin.css"],
    "matches": ["https://www.linkedin.com/*"]
  }]
 
}